{
    "name": "producer/producer",
    "type": "library",
    "description": "Tools for releasing library packages; supports Git, Mercurial, Github, Gitlab, and Bitbucket.",
    "keywords": [
        "library",
        "package",
        "git",
        "hg",
        "mercurial",
        "github",
        "gitlab",
        "bitbucket"
    ],
    "homepage": "https://github.com/producerphp/producer.producer",
    "license": "MIT",
    "authors": [
        {
            "name": "Producer Contributors",
            "homepage": "https://github.com/producerphp/producer.producer/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/log": "~1.0"
    },
    "suggest": {
        "phpunit/phpunit": "Producer will call `phpunit` from your Composer package or global system.",
        "phpdocumentor/phpdocumentor": "Producer will call `phpdoc` from your Composer package or global system."
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "phpdocumentor/phpdocumentor": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Producer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Producer\\": "tests/"
        }
    },
    "bin": [
        "bin/producer"
    ]
}
